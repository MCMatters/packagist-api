{
    "name": "mcmatters/packagist-api",
    "description": "Wrapper for Packagist",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Borzyonok",
            "email": "dima.matters@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "mcmatters/ticl": "dev-master"
    },
    "require-dev": {
        "codeclimate/php-test-reporter": "^0.4.4",
        "phpunit/phpunit": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "McMatters\\Packagist\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "McMatters\\Packagist\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
